pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--[[
jessngage frogduck 
]]
-- main player object
jess = {s=0.5, -- base speed 
     sp=0, -- current sprite
     sp_off=32,
     x=12, y=12,  
     xv=1, yv=1, -- current velocity in x / y direction
     w=5, h=12, -- width / height
     mxv=3, myv=5,
     left=false,
     t_down=false -- touch down
     } -- max x/y velocity

function check_spot(x,y)
 mx = flr(x/8)
 my = flr(y/8)
 pset(mx*8,my*8,9)
 return mget(mx,my) ~= 0
end
-- floor 8-ish
function f8(n)
 return flr(n/8)*8
end

function ck_hz(x,y,h)
 for iy = f8(y), f8(y+h), 8 do
  --print(iy)
  if check_spot(x,iy) then
   return true
  end
 end
 
 return false
end

function ck_vr(x,y,w)
 for ix = f8(x), f8(x+w), 8 do
  --print(ix)
  if check_spot(ix,y) then
   return true
  end
 end
 
 return false 
end

function sign(n)
 if n > 0 then 
  return 1
 elseif n < 0 then 
  return -1
 else 
  return 0 
 end
end

--[[
 moves the player one pixel in
 the horiz/vert direction till
 it either hits a map tile
 or has moved the number of 
 squares indicated by x velocity
]]
function map_collide(o)
 
 --[[
 find x/y of which side the 
 collision checking should 
 be on. (top, bottom, left and
 right side)
 --]] 
 x_off = 0
 if o.xv > 0 then
  x_off = o.w
 end

 y_off = 0
 if o.yv > 0 then
  y_off = o.h
 end

 xi=sign(o.xv)
 yi=sign(o.yv)
 o.t_down = false

 -- y direction
 for i = 1,abs(o.yv) do
  -- check for collision
  -- left or right (see y_off)
  if ck_vr(o.x, o.y+y_off+yi, o.w) then 
   o.yv = 0  
   break 
  end

  o.y+=yi
  
 end
 
 -- x direction
 for i = 1,abs(o.xv) do
  -- check for collision
  -- left or right (see x_off)
  if ck_hz(o.x+x_off+xi, o.y, o.h)
   then o.xv = 0 break end

  o.x += xi 
 end
end

function _draw()
 cls()
 map(
 0, -- which tile to start 
    -- drawing (top left)
    -- in x dir
 0, -- ^- same in y dir
 
 0, -- map place in room x dir
 0, --  ^- same in y dir
 -- one screenfull (16)
 128/8, -- how many cells to draw x
 16     --  ^- same in y dir
 )
 
 draw_o(jess)
end

function draw_o(o)
 if ck_vr(o.x, o.y+o.h+1, o.w) 
 or (o.xv != 0 and flr(o.x) % 2 ==0)
 then
  o.sp = 0
 else
  o.sp = 1
 end
 
 spr(o.sp_off+o.sp*2,
  o.x-5,o.y-3,
  2,2, o.left)
end

function draw_collision_box(o)
 rectfill(
  o.x,
  o.y, 
  o.x+o.w,
  o.y+o.h,
  2) -- color
end

function limit(n, max_n)
 if n > 0 then
  return min(n, max_n)
 elseif n < 0 then
  return max(n, -max_n)
 else
  return 0
 end
end

-- update the nth player
function update_plr(o,n)
 s = o.s
 
 -- ‹ left
 if	btn(0,n)  then
  o.xv-=s 
  o.left=true
 end
 
 -- ‘ right
 if	btn(1,n) then 
  o.xv+=s
  o.left=false 
 end
 
 --  jump?
 if	btnp(4,n) then
  o.yv-=s*10 
 end
 
 -- ƒ down
 if	btn(3,n) then 
  o.yv+=s 
 end
 
 --  ???
 if btn(5,n) then 
  --  
 end
 
 o.yv += 0.5 -- gravity
 
 -- friction if not pushing
 -- ‹ or ‘
 if not(btn(0) or btn(1)) then
  o.xv = o.xv * 0.80
 end
 -- limit how fast the player
 -- can go
 o.xv = limit(o.xv, o.mxv)
 o.yv = limit(o.yv, o.myv)
end


function _update()
 update_plr(jess,0)
 map_collide(jess)
end


