pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
s = 16 -- default size for sprs
gv = 0.1
j = { x=16, y=0, vx=1, vy=1, 
      w=s, h=s }

function vel(o)
 o.x = o.x + o.vx
 o.y = o.y + o.vy
end

function grav(o)
 o.vy = o.vy + gv
end

function draw_o(o)
 sspr(0,0, o.w or s, o.h or s,
  o.x, o.y)
end

function collide(a, b)
 if(
  a.x < b.x + b.w and
  a.x + a.w > b.x and
  a.y < b.y + b.h and
  a.y + a.h > b.y) then
  return true
 else
  return false
 end	
end

--[[ 
mov_x
try to move a single pixel
in one direction

the way we do this is to
check the corners facing
the direction the player
is heading. as long as y0u
check spots that are smaller
than the height/width of a
block it won't miss.
]]
function collide_map(o)
 -- find the top left
 
 local mx = -flr(-o.x/8) * 8  
 local my = -flr(-o.y/8) * 8 
 
 local cr = collide_map_c(o,
  mx, my)
  
 if cl or cr then
  o.vx = 0
  o.vy = 0
  if cr then 
   mset(mx/8, my/8, 4)
  end
  if cl then mset(mx/8, my/8, 4) end
 end
end


function collide_map_c(o, mx, my)
 if (mget(mx/8,my/8) == 0) then
  return false;
 else
  return collide(o, {
   x = mx,
   y = my,
   w = 8,
   h = 8,
  })
 end
end

function _update()
 vel(j)
 grav(j)
 collide_map(j)
 -- camera(x ,y) (how to move)
 
 -- ‹
 if btn(0) then 
  j.vx = j.vx - 1
 end
 
 -- ‘
 if btn(1) then 
  j.vx = j.vx + 1
 end
 
 -- ”
 if btn(2) then 
  j.vy = j.vy - 3
 end 
 
-- btn(‘) j.xv = j.xv + 3
-- btn(”) or j.yv = j.yv - 3
-- btn(ƒ) or j.yv = j.yv + 3
end

function _draw()
 cls()
 draw_o(j)
 map(0,0, 1,1, 16,16)
end
