pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--[[
jessngage frogduck 
]]
-- main player object

function make_plr(num,x,y,sp_off)
 return {
  s=0.5, -- base speed 
  num=num, -- which player 1, 2..)
  sp=0, -- current sprite
  spc=0,-- current sprite's first frame
  frames=1, -- how many frames a animation contains
  sp_off=sp_off, -- an offset for multiple players
  x=x, y=y,  
  xv=1, yv=1, -- current velocity in x / y direction
  w=5, h=12, -- width / height
  mxv=3, myv=5,
  savx = x,
  savy = y,
  left=false,
  ground=false, -- touch down
  } -- max x/y velocity
end

jess = make_plr(0,16,12,32)
gage = make_plr(1,10,-22,64)

function check_spot(x,y)
 mx = flr(x/8)
 my = flr(y/8)
 pset(mx*8,my*8,9)
 val = mget(mx,my) 
 return val >= 3 and val <= 15 
end
-- floor 8-ish
function f8(n)
 return flr(n/8)*8
end

function ck_hz(x,y,h)
 for iy = f8(y), f8(y+h), 8 do
  --print(iy)
  if check_spot(x,iy) then
   return true
  end
 end
 
 return false
end

function ck_vr(x,y,w)
 for ix = f8(x), f8(x+w), 8 do
  --print(ix)
  if check_spot(ix,y) then
   return true
  end
 end
 
 return false 
end

function sign(n)
 if n > 0 then 
  return 1
 elseif n < 0 then 
  return -1
 else 
  return 0 
 end
end

--[[
 moves the player one pixel in
 the horiz/vert direction till
 it either hits a map tile
 or has moved the number of 
 squares indicated by x velocity
]]
function map_collide(o)
 
 --[[
 find x/y of which side the 
 collision checking should 
 be on. (top, bottom, left and
 right side)
 --]] 
 x_off = 0
 if o.xv > 0 then
  x_off = o.w
 end

 y_off = 0
 if o.yv > 0 then
  y_off = o.h
 end

 xi=sign(o.xv)
 yi=sign(o.yv)

 -- y direction
 for i = 1,abs(o.yv) do
  -- check for collision
  -- left or right (see y_off)
  if ck_vr(o.x, o.y+y_off+yi, o.w) then 
   o.yv = 0  
   break 
  end

  o.y+=yi
  
 end
 
 -- x direction
 for i = 1,abs(o.xv) do
  -- check for collision
  -- left or right (see x_off)
  if ck_hz(o.x+x_off+xi, o.y, o.h)
   then o.xv = 0 break end

  o.x += xi 
 end
end

function _draw()
 cls()
 draw_o(jess)
 draw_o(gage)
 map(
 0, -- which tile to start 
    -- drawing (top left)
    -- in x dir
 0, -- ^- same in y dir
 
 0, -- map place in room x dir
 0, --  ^- same in y dir
 -- one screenfull (16)
 16*20, -- how many cells to draw x
 16*20     --  ^- same in y dir
 ) 
end

function draw_o(o)
  spr(o.sp_off+(o.sp+o.spc)*2
  ,o.x-5,o.y-3,
  2,2, o.left)
end

function draw_collision_box(o)
 rectfill(
  o.x,
  o.y, 
  o.x+o.w,
  o.y+o.h,
  2) -- color
end

function limit(n, max_n)
 if n > 0 then
  return min(n, max_n)
 elseif n < 0 then
  return max(n, -max_n)
 else
  return 0
 end
end

function ch_spr(o, n, frames)
 if o.sp ~= n or
 	 o.frames ~= frames then
  o.spc = 0
  o.sp = n 
  o.frames = frames
 end
end

-- update the nth player
function control_plr(o)
 s = o.s
 n = o.num
 
 -- ‹ left
 if	btn(0,n)  then
  o.xv-=s 
  o.left=true
 end
 
 -- ‘ right
 if	btn(1,n) then 
  o.xv+=s
  o.left=false 
 end
 
 -- Ž jump?
 if	btnp(4,n) and o.ground then
  o.yv-=s*20 
  sfx(1)
 end
 
 -- ƒ down
 if	btn(3,n) then 
  o.yv+=10 
 end
 
 -- Ž ???
 if btn(5,n) then 
  --  
 end
 
 o.yv += 0.5 -- gravity
 
 -- friction if not pushing
 -- ‹ or ‘
 if not(btn(0, o.num) 
   or btn(1, o.num)) then
  o.xv = o.xv * 0.80
 end
 -- limit how fast the player
 -- can go
 o.xv = limit(o.xv, o.mxv)
 o.yv = limit(o.yv, o.myv)
end

function update_spr(o)
 o.spc += 1
 if o.spc > o.frames then
  o.spc = 0
 end 
 -- touching ground?
 o.ground = ck_vr(o.x, o.y+o.h+1, o.w)

 if o.ground then
  if btn(0, o.num) or btn(1, o.num) then
   ch_spr(o, 2, 3) 
  else
   ch_spr(o, 0, 0) 
  end
 else
  ch_spr(o, 1, 0) 
 end
end

camx = 0 
camy = 0

function update_cam(p1, p2)
 newcamx=flr((p1.x-4)/128)*128+4
 newcamy=flr((p1.y-4)/128)*128+4
 
 if newcamx ~= camx or
    newcamy ~= camy then
  p1.sav_x = p1.x
  p1.sav_y = p1.y   
  p2.x = p1.x
  p2.y = p1.y
 end
 
 camx = newcamx
 camy = newcamy
 camera(camx, camy)
end

function update_plr(o)
 control_plr(o)
 update_spr(o)
 map_collide(o)
end

function _update()
 update_plr(jess)
 update_plr(gage)
 update_cam(jess, gage)
end

 
